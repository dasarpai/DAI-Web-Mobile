public class A4demo {

	public static void main(String[] args) {
		Graph g = new Graph("Edges.csv");
		//System.out.println(g);

		//calling the method with Oshawa as origin, and 2 as the number of edges should 
		//produce a set with the following items:
		//"Port Perry, 1", "Ajax, 1", "Bowmanville, 1", "Uxbridge, 2", and "Scarborough, 2".

		System.out.println(Graphs1.nearby(g, "Oshawa", 3));
		
		System.out.println(Graphs1.nearby(g, "Aurora", 3));
	}

}
